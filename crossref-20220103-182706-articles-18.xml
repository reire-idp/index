        <titles>
          <title>inclusión de adolescentes latinoamericanos en L’Hospitalet de Llobregat</title>
        </titles>
        <contributors>
          <person_name contributor_role="author" sequence="first" language="es">
            <given_name>Pau</given_name>
            <surname>Gil-Azorín</surname>
            <alt-name>
